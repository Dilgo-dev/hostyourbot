{
  "name": "webhook-service",
  "version": "1.0.0",
  "description": "Service de webhook Discord pour monitoring Kubernetes",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "proto:generate": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./src/proto --grpc_out=grpc_js:./src/proto --plugin=protoc-gen-grpc=`which grpc_tools_node_protoc_plugin` -I ../proto ../proto/k8s.proto"
  },
  "keywords": [
    "discord",
    "webhook",
    "monitoring",
    "kubernetes"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.12.4",
    "@grpc/proto-loader": "^0.7.15",
    "axios": "^1.7.9",
    "dotenv": "^16.4.1",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/node-cron": "^3.0.11",
    "grpc-tools": "^1.12.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
